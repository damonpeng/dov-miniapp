{
  "name": "dov-miniapp",
  "version": "1.1.22",
  "description": "基于JSON配置的小程序静态化框架",
  "keywords": [],
  "author": "damo",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"npm run watch-ts\" \"npm run watch-assets\"",
    "watch-ts": "npx tsc --watch",
    "watch-assets": "nodemon",
    "build": "npx tsc && npm run copyfiles",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "copyfiles": "copyfiles -u 1 'src/**/*.{wxml,wxss,json,js}' dist/"
  },
  "dependencies": {
    "@vant/weapp": "^1.11.5"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "miniprogram-api-typings": "^4.0.5",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.2"
  },
  "miniprogram": "dist",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/damonpeng/dov-miniapp.git"
  },
  "homepage": "https://github.com/damonpeng/dov-miniapp#readme"
}
