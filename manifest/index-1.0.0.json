{
  "struct": "site",
  "title": "dov",
  "icon": "https://s.dayanmo.com/dov/static/dov.jpg",
  "plugins": {
    "txc": {
      "productId": 0
    }
  },
  "settings": {
    "shareInfo": {
      "title": "${default}",
      "path": "${default}",
      "image": "${default}"
    }
  },
  "items": [
