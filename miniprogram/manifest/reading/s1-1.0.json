      },
      "items": [
        {
          "struct": "component",
          "type": "cell",
          "title": "唐诗",
          "items": [
            {
              "title": "咏鹅",
              "author": "[唐] 骆宾王",
              "contents": [
                "鹅，鹅，鹅",
                "曲项向天歌",
                "白毛浮绿水",
                "红掌拨清波"
              ]
            },
            {
              "title": "江南",
              "author": "[两汉] 汉乐府",
              "contents": [
                "江南可采莲",
                "莲叶何田田",
                "鱼戏莲叶间",
                "鱼戏莲叶东",
                "鱼戏莲叶西",
                "鱼戏莲叶南",
                "鱼戏莲叶北"
              ]
            },
            {
              "title": "画",
              "author": "[唐] 王维",
              "contents": [
                "远看山有色",
                "近听水无声",
                "春去花还在",
                "人来鸟不惊"
              ]
            },
            {
              "title": "悯农二首·其二",
              "author": "[唐] 李绅",
              "contents": [
                "锄禾日当午",
                "汗滴禾下土",
                "谁知盘中餐",
                "粒粒皆辛苦"
              ]
            },
            {
              "title": "古朗月行（节选）",
              "author": "[唐] 李白",
              "contents": [
                "小时不识月",
                "呼作白玉盘",
                "又疑瑶台镜",
                "飞在青云端"
              ]
            },
            {
              "title": "风",
              "author": "[唐] 李峤",
              "contents": [
                "解落三秋叶",
                "能开二月花",
                "过江千尺浪",
                "入竹万竿斜"
              ]
            },
            {
              "title": "春晓",
              "author": "[唐] 孟浩然",
              "contents": [
                "春眠不觉晓",
                "处处闻啼鸟",
                "夜来风雨声",
                "花落知多少"
              ]
            },
            {
              "title": "赠汪伦",
              "author": "[唐] 李白",
              "contents": [
                "李白乘舟将欲行",
                "忽闻岸上踏歌声",
                "桃花潭水深千尺",
                "不及汪伦送我情"
              ]
            },
            {
              "title": "静夜思",
              "author": "[唐] 李白",
              "contents": [
                "床前明月光",
                "疑是地上霜",
                "举头望明月",
                "低头思故乡"
              ]
            },
            {
              "title": "寻隐者不遇",
              "author": "[唐] 贾岛",
              "contents": [
                "松下问童子",
                "言师采药去",
                "只在此山中",
                "云深不知处"
              ]
            },
            {
              "title": "池上",
              "author": "[唐] 白居易",
              "contents": [
                "小娃撑小艇",
                "偷采白莲回",
                "不解藏踪迹",
                "浮萍一道开"
              ]
            },
            {
              "title": "小池",
              "author": "[宋] 杨万里",
              "contents": [
                "泉眼无声惜细流",
                "树阴照水爱晴柔",
                "小荷才露尖尖角",
                "早有蜻蜓立上头"
              ]
            },
            {
              "title": "画鸡",
              "author": "[明] 唐寅",
              "contents": [
                "头上红冠不用裁",
                "满身雪白走将来",
                "平生不敢轻言语",
                "一叫千门万户开"
              ]
            }
          ]
        }
      ]
    }
  ]
}