  "items": [
    {
      "struct": "module",
      "title": "唐诗",
      "router": "/index/poem2/list",
      "extends": {},
      "items": [
        {
          "struct": "component",
          "type": "cell",
          "title": "唐诗",
          "items": [
            {
              "title": "梅花",
              "author": "[宋] 王安石",
              "contents": [
                "墙角数枝梅",
                "凌寒独自开",
                "遥知不是雪",
                "为有暗香来"
              ]
            },
            {
              "title": "小儿垂钓",
              "author": "[唐] 胡令能",
              "contents": [
                "蓬头稚子学垂纶",
                "侧坐莓苔草映身",
                "路人借问遥招手",
                "怕得鱼惊不应人"
              ]
            },
            {
              "title": "登鹳雀楼",
              "author": "[唐] 王之涣",
              "contents": [
                "白日依山尽",
                "黄河入海流",
                "欲穷千里目",
                "更上一层楼"
              ]
            },
            {
              "title": "望庐山瀑布",
              "author": "[唐] 李白",
              "contents": [
                "日照香炉生紫烟",
                "遥看瀑布挂前川",
                "飞流直下三千尺",
                "疑是银河落九天"
              ]
            },
            {
              "title": "江雪",
              "author": "[唐] 柳宗元",
              "contents": [
                "千山鸟飞绝",
                "万径人踪灭",
                "孤舟蓑笠翁",
                "独钓寒江雪"
              ]
            },
            {
              "title": "夜宿山寺",
              "author": "[唐] 李白",
              "contents": [
                "危楼高百尺",
                "手可摘星辰",
                "不敢高声语",
                "恐惊天上人"
              ]
            },
            {
              "title": "敕勒歌",
              "author": "[南北朝] 乐府诗集",
              "contents": [
                "敕勒川，阴山下",
                "天似穹庐，笼盖四野",
                "天苍苍，野茫茫",
                "风吹草低见牛羊"
              ]
            },
            {
              "title": "村居",
              "author": "[清] 高鼎",
              "contents": [
                "草长莺飞二月天",
                "拂堤杨柳醉春烟",
                "儿童散学归来早",
                "忙趁东风放纸鸢"
              ]
            },
            {
              "title": "咏柳",
              "author": "[唐] 贺知章",
              "contents": [
                "碧玉妆成一树高",
                "万条垂下绿丝绦",
                "不知细叶谁裁出",
                "二月春风似剪刀"
              ]
            },
            {
              "title": "晓出净慈寺送林子方",
              "author": "[宋] 杨万里",
              "contents": [
                "毕竟西湖六月中",
                "风光不与四时同",
                "接天莲叶无穷碧",
                "映日荷花别样红"
              ]
            },
            {
              "title": "绝句",
              "author": "[唐] 杜甫",
              "contents": [
                "两个黄鹂鸣翠柳",
                "一行白鹭上青天",
                "窗含西岭千秋雪",
                "门泊东吴万里船"
              ]
            },
            {
              "title": "悯农二首·其一",
              "author": "[唐] 李绅",
              "contents": [
                "春种一粒粟",
                "秋收万颗子",
                "四海无闲田",
                "农夫犹饿死"
              ]
            },
            {
              "title": "舟夜书所见",
              "author": "[清] 查慎行",
              "contents": [
                "月黑见渔灯",
                "孤光一点萤",
                "微微风簇浪",
                "散作满河星"
              ]
            }
          ]
        }
      ]
    }
  ]
}