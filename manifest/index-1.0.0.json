      "items": [
        {
          "struct": "page",
          "title": "五言绝句",
          "router": "/index/poem1",
          "script": "https://s.dayanmo.com/dov/manifest/reading/s1-${version}.json",
          "version": "1.3.0"
        },
        {
          "struct": "page",
          "title": "二年级",
          "router": "/index/poem2",
          "script": "https://s.dayanmo.com/dov/manifest/reading/s2-${version}.json",
          "version": "1.3.0"
        }
      ]
    },
    {
      "struct": "channel",
      "router": "/demo",
      "title": "示例",
      "icon": "/static/icon/listening-default.png",
      "iconActive": "/static/icon/listening.png",
      "items": [
        {
          "struct": "page",
          "title": "图文内容",
          "router": "/demo/index",
          "script": "https://s.dayanmo.com/dov/manifest/demo/list-${version}.json",
          "version": "1.0.0"
        }
      ]
    }
  ]
}